						font-size: 12px;
						font-weight: normal;
						font-stretch: normal;
						font-style: normal;
						line-height: normal;
						letter-spacing: normal;
						text-align: center;
						color: #ffffff;
						border-bottom: 0;
						padding: 0;
						display: flex;
						flex-direction: column;
						align-items: center;
						justify-content: flex-end;
						min-height: 60px;
						text-transform: inherit;
						@media ($max: 767px) {
							min-height: 60px;
							font-size: 18px;
							letter-spacing: 0.14px;
						}
						@media ($max: 479px) {
							font-size: 12px;
							min-height: 40px;
							letter-spacing: 0.14px;
						}
					}
					&.tier-status {
						display: none;
						font-family: $theme-font-family-Roboto;
						font-size: 16px;
						font-weight: 500;
						font-stretch: normal;
						font-style: normal;
						line-height: normal;
						letter-spacing: normal;
						text-align: center;
						color: #ffffff;
						border: 0;
						min-height: 38px;
						text-transform: uppercase;
						background-color: rgba(198, 150, 146, 0.9);
						@media($min: $small) {
							font-size: 24px;
							min-height: 44px;
						}
						@media($min:$medium) {
							font-size: 16px;
							min-height: 45px;
						}
						.next-tier{
							font-size: 12px;
						}
					}
					&.tier-earn {
						display: none;
						font-family: $theme-font-family-Roboto;
						font-size: 13px;
						min-height: 0;
						border-bottom: 0;
						padding: 0 3px;
						display: flex;
						align-items: center;
						justify-content: center;
						font-size: 14px;
						font-weight: normal;
						font-stretch: normal;
						font-style: normal;
						line-height: normal;
						letter-spacing: 0.14px;
						text-align: center;
						color: #1c1c1c;
						@media ($min: $medium) {
							font-size: 12px;
						}
					}
					&.tier-name {
						font-family: $theme-font-family-Roboto;
						font-size: 14px;
						font-weight: 500;
						font-stretch: normal;
						font-style: normal;
						line-height: normal;
						letter-spacing: normal;
						text-align: center;
						color: #ffffff;
						border: 0;
						display: flex;
						text-transform: uppercase;
						align-items: center;
						padding-bottom: 0;
						min-height: 35px;
						@media ($min: $small) {
							font-size: 22px;
							min-height: 55px;
						}
						@media ($min: $medium) {
							font-size: 15px;
							letter-spacing: normal;
							min-height: 60px;
							padding-bottom: 12px;
						}
						@media ($min: $large) {
							font-size:16px;
						}
					}
					&.tier-name {
						border: solid 1px rgba(147, 137, 134, 0.2);
						border-top: 0;
						border-left: 0;
						border-right: 0;
					}
					&.tier-value {
						border-top:0;
					}
				}
			}
			&.logged-in {
