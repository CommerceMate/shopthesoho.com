					&:not(.swell-tier-active){
						.tier-list{
							color: #333;
							.circle{
								background-color: #333;
							}
						}
					}
					.tier-list{
						.tier-status {
							min-height: 38px;
							@media ($min: $small) {
								min-height: 44px;
							}
							@media ($min: $medium) {
								min-height: 43px;
							}
						}
					}
				}
				.slick-dots{
					top: -32px;
					@media($min: $small) {
						top: -40px;
					}	
				}
			}
			.slick-dots {
				left: 0;
				right: 0;
				bottom: auto;
				top: -30px;
				display: block !important;
				@media($min: $small) {
					top: -40px;
				}
				button {
					&:before {
						display: none;
					}
				}
				li {
					width: 12px;
					height: 12px;
					background-color: rgba(0, 0, 0, 0.5);
					margin: 0 4px;
					@media($min: $small) {
