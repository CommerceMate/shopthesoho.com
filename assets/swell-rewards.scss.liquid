					width: auto;
					flex: 0.6;
				}
				@media($min:$large) {
					flex: 1;
				}
				.swell-tier-list {
					border: 0;
					padding: 0;
					.tier-list {
						display: flex;
						align-items: center;
						border: 0;
						border-bottom: solid 1px rgba(147, 137, 134, 0.2);
						border-right: solid 1px rgba(147, 137, 134, 0.2);
						padding-left: 5px;
						@media($min:$small) {
							padding-left: 10px;
						}
						@media($min:$medium) {
							padding-left: 10px;
						}
						@media($min:$large) {
							padding-left: 30px;
						}
						&.tier-name {
							padding-bottom: 0;
							border-right: 0;
							align-items: flex-start !important;
							border-left: 0;
							color: #333333;
						}
						&.tier-value {
							min-height: 40px;
							border-left: solid 1px rgba(147, 137, 134, 0.2);
							border-top: solid 1px rgba(147, 137, 134, 0.2);
							@media($min: $small) {
								min-height: 60px;
							}
							@media($min:$medium) {
								min-height: 60px;
							}
						}
					}
					&:nth-child(1) {
						.tier-name {
							border: solid 1px rgba(147, 137, 134, 0.2);
							border-top: 0;
						}
					}
					&:nth-child(1) {
						.tier-value {
							border-bottom: none;
						}
					}
				}
				.tier-name {
					display: flex;
					align-items: center !important;
				}
				@media ($min: $medium) {
					margin-top: 0;
				}
			}
			.swell-tier-table-body {
				flex: 1;
				display: flex;
				margin-top: 30px;
				width: 50%;
				@media($min: $small) {
					flex: 1;
				}
				@media($min: $medium) {
					max-width: 500px;
					margin-top: 0;
					flex: 1;
				}
				@media($min:900px) {
					max-width: 100%;
				}
				@media($min: $large) {
					flex:1.3;
				}
				.swell-tier-list {
					.tier-list{
						color: $colorWhite;
						.circle{
							background-color: $colorWhite;
						}
						&:last-child{
							border-bottom: 0;
						}
					}
					&.tier_1 {
						.tier-list {
							background-color: rgba(198, 150, 146, 0.9);
							&.tier-status{
								background-color: rgba(198, 150, 146, 0.9);
							}
						}
					}
					&.tier_0 {
						.tier-list{
							background-color: rgba(198, 150, 146, 0.7);
							&.tier-status{
								background-color: rgba(198, 150, 146, 0.7);
							}
						}
					}
					&.tier_2 {
						.tier-list{
							background-color: #c69692;
							&.tier-status{
								background-color: #c69692;
							}
						}
					}
				}
				.slick-list {
					width: 100%;
					.slick-track {
						width: 100%;
						.slick-slide{
							margin:0;
						}
					}
				}
				.swell-tier-list {
					flex: 1;
					&.tier-active {
						.tier-list {
							&.tier-status {
								background-color: transparent;
							}
							&.tier-name {}
							.circle {
								background-color: $colorWhite;
							}
						}
					}
					.tier-list {
						display: flex;
						align-items: center;
						justify-content: center;
					}
				}
				&.slick-slider {
					position: relative;
				}
			}
			.swell-tier-list {
				position: relative;
				.tier-list {
					padding: 0px 8px;
					font-family: $theme-font-family-Roboto;
					font-size: 14px;
					font-weight: normal;
					font-stretch: normal;
					font-style: normal;
					letter-spacing: normal;
					text-align: left;
					color: #333333;
					min-height: 90px;
					border-bottom: solid 1px rgba(0, 0, 0, 0.05);
					border-top: 0;
					line-height: normal;
					@media ($min: $small) {
						font-size: 22px;
						min-height: 100px;
					}
					@media ($min: $medium) {
