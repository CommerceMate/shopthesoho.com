@font-face {
	font-family: 'swell-rewards';
	src:  url('swell-rewards.eot?e79uh3');
	src:  url('swell-rewards.eot?e79uh3#iefix') format('embedded-opentype'),
	url('swell-rewards.ttf?e79uh3') format('truetype'),
	url('swell-rewards.woff?e79uh3') format('woff'),
	url('swell-rewards.svg?e79uh3#swell-rewards') format('svg');
	font-weight: normal;
	font-style: normal;
	font-display: block;
}

[class^="swell-icon-"], [class*=" swell-icon-"] {
	/* use !important to prevent issues with browser extensions that change fonts */
	font-family: 'swell-rewards' !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.swell-icon-birthday-reward:before {
	content: "\e900";
	color: #333333;
}
.swell-icon-create-an-account:before {
	content: "\e901";
	color: #333333;
}
.swell-icon-follow-on-facebook:before {
	content: "\e902";
	color: #333333;
}
.swell-icon-follow-on-instagram:before {
	content: "\e903";
	color: #333333;
}
.swell-icon-per-1-spent:before {
	content: "\e904";
	color: #333333;
}
.swell-icon-punch-card:before {
	content: "\e905";
	color: #333333;
}
.swell-icon-review-with-photo:before {
	content: "\e906";
	color: #333333;
}
.swell-icon-sign-up-for-youtube:before {
	content: "\e907";
	color: #333333;
}
.swell-icon-write-a-product-review:before {
	content: "\e908";
	color: #333333;
}

@font-face {
	font-family: 'Modena Script';
	src: url('ModenaScript-Regular.woff2') format('woff2'),
	url('ModenaScript-Regular.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}
$theme-font-family-Roboto:'Roboto', sans-serif;
$theme-font-family-Modena: 'Modena Script';
$min: min-width;
$max: max-width;
$small: 480px;
$medium: 768px;
$large: 1024px;

/* Colors Variable Start */

$colorBlack: #000;
$colorWhite: #fff;
$colorDarkGrey: #252525;

/* Colors Variable End */

.swell-Gameday {
	img {
		width: 100%;
		height: 100%;
		object-position: center top;
		vertical-align: -webkit-baseline-middle;
		object-fit: cover;
	}
	.swell-desktop {
		display: none;
		@media ($min: $medium) {
			display: block;
		}
	}
	.swell-mobile {
		display: block;
		@media ($min: $medium) {
			display: none;
		}
	}
	.swell-container {
		max-width: 1200px;
		margin: 0 auto;
		padding: 0 5px;
	}
	a {
		text-decoration: none;
		cursor: pointer;
	}
	ul {
		margin: 0;
		padding: 0;
	}
	li {
		list-style-type: none;
		margin: 0;
		padding: 0;
	}
	/* Generic Buttons Code Start */
	.swell-link-list {
		display: flex;
		justify-content: center;
		width: 100%;
		align-items: center;
		margin: 0 auto;
		flex-direction: column;
		@media ($min: $medium) {
			max-width: 390px;
			flex-direction: row;
		}
		.swell-link {
			max-width: 122px;
			width: 100%;
			position: relative;
			margin-bottom: 20px;
			max-width: 255px;
			@media ($min: $small) {
				margin-bottom: 25px;
				max-width: 400px;
			}
			@media ($min: $medium) {
				flex: 1;
				max-width: 147px;
				margin-bottom: 0;
			}
			a,
			.swell-link-history {
				font-family: $theme-font-family-Roboto;
				font-size: 12px;
				font-weight: normal;
				font-stretch: normal;
				font-style: normal;
				line-height: normal;
				letter-spacing: normal;
				text-align: center;
				color: #ffffff;
				cursor: pointer;
				display: flex;
				align-items: center;
				justify-content: center;
				width: 100%;
				text-decoration: none;
				text-transform: uppercase;
				margin-bottom: 0;
				background-color: #f2c551;
				border: solid 1px #f2c551;
				height: 45px;
				@media ($min: $small) {
					font-size: 19px;
					height: 55px;
				}
				@media ($min: $medium) {
					font-size: 12px;
					height: 42px;
				}
				&:hover {
					background: transparent;
					border-color: #f2c551;
					color: #f2c551;
				}
			}
		}
	}
	/* Generic Buttons Code End */
	.swell-heading {
		font-family: $theme-font-family-Roboto;
		font-size: 30px;
		font-weight: 500;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		text-align: center;
		color: #333333;
		margin: 0;
		margin-bottom: 8px;
		text-transform: uppercase;
		@media ($min: $small) {
			font-size: 40px;
			margin-bottom: 12px;
		}
		@media ($min: $medium) {
			font-size: 30px;
			margin-bottom: 20px;
		}
	}
	.swell-header-details {
		font-family: $theme-font-family-Roboto;
		font-size: 16px;
		font-weight: 500;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		text-align: center;
		color: #333333;
		padding: 0;
		margin: 0;
		@media($min: $small) {
			font-size: 24px;
		}
		@media($min:$medium) {
			font-size: 16px;
		}
	}
	.swell-details {
		font-family: $theme-font-family-Roboto;
		font-size: 14px;
		font-weight: normal;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		text-align: center;
		color: #333333;
		margin: 0 auto;
		@media($min: $small) {
			font-size: 22px;
			max-width: 387px;
		}
		@media($min:$medium) {
			font-size: 14px;
			max-width:100%;
		}
	}
	/*generic code ends */
	#banner {
		display: flex;
		justify-content: center;
		align-items: center;
		flex-direction: column;
		position: relative;
		background-image: url(swell-banner.png);
		background-size: cover;
		background-position: center;
		@media($min: $medium) {
			flex-direction: row;
			min-height: 63vh;
		}
		@media($min: $large) {
			min-height:85vh;
		}
		@media($min: 1400px) {
			min-height:81vh;
		}
		@media($min: 1920px) {
			min-height:102vh;
		}
		.banner-main-header{

			width: 100%;
			position: absolute;
			top: 50px;
			@media($min:$small) {
				top: 100px;
			}
			@media($min:$medium) {
				top: 10px;
			}
			@media($min:$large) {
				top: 35px;
			}
			.banner-main-heading{
				font-family: $theme-font-family-Roboto;
				font-size: 40px;
				font-weight: bold;
				font-stretch: normal;
				font-style: normal;
				line-height: normal;
				letter-spacing: 0.5px;
				text-align: center;
				color: #ffffff;
				@media($min:$small) {
					font-size: 55px;
				}
				@media($min:$medium) {
					letter-spacing: 1px;
					font-size: 50px;
				}
				@media($min:$large) {
					font-size:74px;
				}
			}
		}
		.caption-content-holder {
			display: flex;
			flex-direction: column;
			justify-content: center;
			align-items: flex-end;
			width: 100%;
			max-width: 950px;
			margin: 0 auto;
			position: absolute;
			bottom: 15%;
			@media($min: $small) {
				bottom: 12%;
			}
			@media($min:$medium) {
				height: 100%;
				position:unset;
			}
			@media($min:$large) {
				max-width: 1450px;
			}
			@media($min:1450px) {
				max-width:1800px;
			}
			.caption-content {
				display: flex;
				flex-direction: column;
				justify-content: center;
				align-items: center;
				width: 100%;
				@media($min:$medium) {
					padding-right: 2px;
					width: auto;
				}
				@media($min:$large) {
					padding-right: 24px;
				}
				@media($min:1200px) {
					padding-right: 23px;
				}
				.banner-header {
					font-family:$theme-font-family-Modena;
					font-size: 100px;
					font-weight: normal;
					font-stretch: normal;
					font-style: normal;
					line-height: 108px;
					letter-spacing: normal;
					text-align: center;
					color: #ffffff;
					text-transform: lowercase;
					margin-bottom: 15px;
					@media($min: $small) {
						font-size: 150px;
						line-height: 155px;
						margin-bottom: 18px;
					}
					@media($min:$medium) {
						font-size: 97px;
						line-height: 82px;
						margin-bottom: 13px;
					}
					@media($min:$large) {
						font-size: 120px;
						line-height: 155px;
					}
					@media($min:1200px) {
						font-size:160px;
					}
					@media($min:1450px) {
						font-size:182px;
					}
				}
				.banner-text {
					font-family: $theme-font-family-Roboto;
					font-size: 14px;
					font-weight: normal;
					font-stretch: normal;
					font-style: normal;
					line-height: normal;
					letter-spacing: normal;
					text-align: center;
					color: #ffffff;
					margin: 0 auto;
					max-width: 255px;
					margin-bottom: 30px;
					@media($min: $small) {
						font-size: 22px;
						max-width: 400px;
						margin-bottom: 40px;
					}
					@media($min:$medium) {
						font-size: 12px;
						max-width: 270px;
						margin-bottom: 40px;
					}
					@media($min: 850px) {
						font-size: 12px;
						max-width: 331px;
						margin-bottom: 40px;
					}
					@media($min:$large) {
						font-size: 13px;
						max-width: 330px;
						margin-bottom: 50px;
					}
					@media($min: 1250px) {
						font-size: 14px;
						max-width: 390px;
						margin-bottom: 50px;
					}
					@media($min: 1450px) {
						font-size: 14px;
						max-width: 420px;
						margin-bottom: 50px;
					}
				}
				.swell-link-list {
					margin-bottom: 8px;
					@media($min: $small) {
						margin-bottom: 10px;
					}
					@media($min:$medium) {
						margin-bottom: 17px;
					}
				}
				.banner-login {
					font-family: $theme-font-family-Roboto;
					font-size: 14px;
					font-weight: normal;
					font-stretch: normal;
					font-style: normal;
					line-height: normal;
					letter-spacing: normal;
					text-align: center;
					color: #ffffff;
					max-width: 100%;
					@media($min: $small) {
						font-size: 22px;
						margin: 0 auto;
					}
					@media($min:$medium) {
						font-size: 12px;
					}
					@media($min:$large) {
						font-size: 14px;
					}
					.signin-text {
						font-weight: 500;
						color: #ffffff;
						border-bottom: 1px solid $colorWhite;
					}
				}
			}
		}
	}
	/*banner section ends*/
	.login{
		background-color: #f5f5f5;
		padding: 50px 0;
		margin-bottom: 40px;
		@media($min:$small) {
			margin-bottom: 60px;
			padding: 60px 0;
		}
		@media($min:$medium) {
			padding: 120px 0;
			margin-bottom: 90px;
		}
		.login-header{
			.login-heading{
				font-family: $theme-font-family-Roboto;
				font-size: 30px;
				font-weight: 500;
				font-stretch: normal;
				font-style: normal;
				line-height: normal;
				letter-spacing: normal;
				text-align: center;
				color: #333333;
				margin-bottom: 25px;
				text-transform: uppercase;
				@media($min:$small) {
					font-size: 40px;
					margin-bottom: 35px;
				}
				@media($min:$medium) {
					font-size: 30px;
					margin-bottom: 30px;
				}
			}
		}
		.swell-link-list{
			justify-content: space-between;
			max-width: 100%;
			margin: 0 auto;
			@media($min:$medium) {
				max-width: 340px;
			}
			.swell-link{
				margin-bottom: 10px;
				max-width: 278px;
				@media($min:$small) {
					max-width: 370px;
					margin-bottom: 15px;
				}
				@media($min:$medium) {
					max-width: 163px;
					margin-bottom:0;
				}
				a{
					background-color: #333333;
					border-color: #333;
					color: #fff;
					&:hover {
						background: transparent;
						border-color: #333;
						color: #333;
					}
				}
			}
		}
	}
	#how-it-work {
		padding: 50px 0 0;
		@media ($min: $small) {
			padding: 60px 0 0;
		}
		@media ($min: $medium) {
			padding: 110px 0;
		}
		.how-it-work-holder {
			.how-it-work-heading {
				.swell-heading {

				}
			}
			.swell-details{
				max-width: 250px;
				margin-bottom: 45px;
				@media($min:$small) {
					margin-bottom: 60px;
					max-width: 388px;
				}
				@media($min:$medium) {
					max-width: 505px;
					margin: 0 auto;
					margin-bottom: 65px;
				}
			}
			.how-it-work-content-holder {
				max-width: 450px;
				margin: 0 auto;
				display: flex;
				flex-direction: column;
				flex-wrap: wrap;
				justify-content:center;
				@media ($min:$medium) {
					max-width: 950px;
					flex-wrap: nowrap;
					flex-direction: row;
				}
				.how-it-work-content {
					flex-basis: 100%;
					margin-bottom: 45px;
					@media ($min:$small) {
						margin-bottom: 50px;
					}
					@media ($min:$medium) {
						flex-basis:33.3%;
						margin-bottom: 0;
					}
					.hiw-image{
						max-width: 130px;
						margin: 0 auto;
						margin-bottom: 20px;
						@media($min:$small) {
							max-width: 180px;
							margin-bottom: 30px;
						}
						@media($min:$medium) {
							margin-bottom: 25px;
							max-width: 110px;
						}
					}
					.swell-header-details{
						text-transform: uppercase;
						@media($min:$medium) {
							font-size: 14px;
							padding-right: 30px;
						}
					}
				}
			}
		}
	}
	/*how it work section ends*/
	#way-to-earn-points{
		margin-bottom: 40px;
		@media($min:$small) {
			margin-bottom: 60px;
		}
		@media($min:$medium) {
			margin-bottom: 110px;
		}
		.swell-container{
			padding: 0 10px;
			@media($min:$small) {
				padding: 0 15px;
			}
			@media($min:$medium) {
				padding: 0 5px;
			}
			.way-to-earn-points-header{
				.swell-heading{
					@media($max:479px) {
						font-size: 27px;
						margin-bottom: 35px;
					}
					@media($min:$medium) {
						margin-bottom:50px;
					}
				}
			}
			.campaign-holder{
				display: flex;
				flex-wrap: wrap;
				justify-content: center;
				.swell-campaign{
					flex-basis: 50%;
					display: flex;
					justify-content: center;
					position: relative;
					@media($min:$medium) {
						flex-basis: 20%;
					}
					&:nth-child(2n) {
						.swell-campaign-content{
							margin-right: 0;
							@media($min:$medium) {
								margin-right: 5px;
							}
						}
					}
					&:nth-child(5n) {
						.swell-campaign-content{
							@media($min:$medium) {
								margin-right: 0;
							}
						}
					}
					&:nth-child(-n+2){
						@media($max:767px) {
							.swell-campaign-content{
								margin-top:0;
							}
						}
					}
					&:nth-child(-n+5){
						@media($min:$medium) {
							.swell-campaign-content{
								margin-top: 0;
							}
						}
					}
					.swell-campaign-hover{
	          position: absolute;
	          height: 100%;
	          width: 100%;
	          display: flex;
	          justify-content: center;
	          align-items: center;
	          background-color: rgba(0,0,0,0.6);
	          z-index: 9;
	          .swell-campaign-hover-content{
	            width: 100%;
	            .swell-link-list{
	              width: 100%;
	              display: flex;
	              justify-content: center;
	              align-items: center;
	              flex-direction: column;
	              padding: 20px;
	              .swell-link{
	                width: 100%;
	                flex-basis: 100%;
	                display: block;
	                a{
	                  display: flex;
	                  justify-content: center;
	                  align-items: center;
	                  width: 100%;
	                  min-height: 40px;
	                  &:hover {
	                    color: #fff;
	                    border-color: #fff;
	                  }
	                }
	                &:first-child {
	                  margin-bottom: 10px;
	                }
	              }
	            }
	          } 
	        }
					.swell-campaign-content{
						border: solid 1px rgba(225, 225, 225, 0.6);
						display: flex;
						flex-direction: column;
						justify-content: center;
						align-items: center;
						width: 100%;
						padding: 10px 0 2px;
						margin-right: 7px;
						margin-top: 7px;
						@media($min:$small) {
							padding: 17px 0 2px;
							margin-right: 10px;
							margin-top: 10px;
						}
						@media($min:$medium) {
							margin-right:5px;
							padding: 20px 0;
							margin-top:5px;
						}
						@media($min:$large) {
							padding: 45px 0;
						}
						.campaign-image{
							max-width: 100px;
							@media($min:$small) {
								max-width: 150px;
							}
							@media($min:$medium) {
								max-width: 100px;
							}
						}
						.swell-campaign-icon{
							font-size: 35px;
							margin-bottom: 5px;
							@media($min:$small) {
								font-size: 45px;
								margin-bottom: 7px;
							}
							@media($min:$medium) {
								margin-bottom: 10px;
								font-size:35px;
							}
							&:last-child{
								margin-bottom: 0;
							}
							.swell-campaign-icon-content{
							}
						}
						.swell-campaign-header{
							text-align: center;
							min-height: 50px;
							@media($min:$medium) {
								min-height: 39px;
							}
							.swell-details{
								.campaign-drop{
									display: block;
									font-size: 12px;
								}
							} 
						}
						.swell-campaign-points{
							margin-bottom: 2px;
							@media($min:$small) {
								margin-bottom: 5px;
							}
							@media($min:$medium) {
								margin-bottom:0;
							}
							.swell-header-details{
								text-transform: uppercase;
								@media($min:$small) {
								}
								@media($min:$medium) {
								}
							}
						}
					}
				}
			}
		}
	}
	/*campaign section ends*/
	.vip-table-section {
		max-width: 1620px;
		text-align: center;
		width: 100%;
		margin: 0 auto 35px;
		@media ($min: $small) {
			margin: 0 auto 40px;
		}
		@media ($min: $medium) {
			margin-bottom: 90px;
		}
		&.logged-in{
			.swell-heading{
				margin-bottom: 40px;
				@media($min:$small) {
					margin-bottom: 60px;
				}
				@media($min:$medium) {
					margin-bottom:20px;
				}
			}
			.swell-tier-table{
				.swell-tier-table-body{
					.slick-dots{
						top: -41px;
						@media($min:$small) {
							top: -47px;
						}
					}
				}
			}
		}
		.swell-heading{
			@media($max:479px) {
				font-size: 26px;
			}
		}
		.tier-text {
			max-width: 234px;
			margin: 0 auto;
			margin-bottom: 10px;
			@media($min: $small) {
				max-width: 345px;
				margin-bottom: 15px;
			}
			@media($min:$medium) {
				max-width: 100%;
				margin-bottom: 30px;
			}
			.swell-details {
				color: #020202;
				text-align: center;
			}
		}
		.swell-tier-table {
			padding: 0;
			display: flex;
			justify-content: center;
			padding: 15px 5px 0;
			max-width: 310px;
